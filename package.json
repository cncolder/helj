  "scripts": {
    "build": "NODE_ENV=production webpack --progress",
    "deploy": "pm2 deploy package.json production",
    "stage": "pm2 deploy package.json staging",
    "start": "pm2-dev --raw package.json",
    "stop": "pm2 kill",
    "test": "mocha"
  },
  "apps": [{
    "env": {
      "DEBUG": "app:*,koa:*,socket.io:*,-socket.io:client,-follow-redirects",
      "DEBUG_COLORS": 1,
      "HOST": "0.0.0.0",
      "MONGO_URL": "mongodb://127.0.0.1:27017/helj?auto_reconnect=true",
      "NODE_ENV": "development",
      "PORT": 3000
    },
    "env_staging": {
      "DEBUG": "app:*",
      "HOST": "127.0.0.1",
      "MONGO_URL": "mongodb://127.0.0.1:27017/heljstage?auto_reconnect=true",
      "NODE_ENV": "staging",
      "PORT": 3333
    },
    "env_production": {
      "DEBUG": "app:*",
      "HOST": "127.0.0.1",
      "NODE_ENV": "production",
      "PORT": 7777
    },
    "ignore_watch": [
      "[\\/\\\\]\\./",
      ".git",
      "client",
      "node_modules",
      "gulpfile",
      "public",
      "test",
      "views"
    ],
    "instances": "1",
    "name": "helj",
    "script": ".",
    "watch": false
  }],
  "deploy": {
    "staging": {
      "host": "139.129.27.109",
      "path": "/var/www/heljstage",
      "post-deploy": "cnpm install --production --unsafe-perm --no-bin-links ; pm2 startOrRestart package.json --env staging ; cnpm prune",
      "ref": "origin/dev",
      "repo": "/var/git/helj",
      "user": "root"
    },
    "production": {
      "host": "helj.co",
      "path": "/var/www/helj",
      "post-deploy": "cnpm install --production --unsafe-perm --no-bin-links ; pm2 startOrRestart package.json --env production ; cnpm prune",
      "ref": "origin/master",
      "repo": "/var/git/helj",
      "user": "root"
    }
  }
